{
	"name": "remanso-cli",
	"version": "0.1.0",
	"type": "module",
	"bin": {
		"remanso": "dist/index.js"
	},
	"files": [
		"dist",
		"README.md"
	],
	"main": "./dist/index.js",
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"lint": "biome lint --write src",
		"format": "biome format --write src",
		"build": "bun build src/index.ts --target node --outdir dist",
		"test": "bun test",
		"dev": "bun run build && bun link",
		"deploy": "bun run build && bun run release-it"
	},
	"dependencies": {
		"@atproto/api": "^0.18.17",
		"@clack/prompts": "^1.0.0",
		"cmd-ts": "^0.14.3",
		"glob": "^13.0.0",
		"mime-types": "^2.1.35",
		"minimatch": "^10.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@types/mime-types": "^3.0.1",
		"@types/node": "^20",
		"release-it": "^19.2.4"
	}
}
